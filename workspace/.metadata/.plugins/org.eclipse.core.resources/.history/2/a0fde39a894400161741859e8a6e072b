
public class ExampDriver {
	public static void main(String[] args){
		StringBuilder stB = new StringBuilder(); //String Builders are an easier way to help manage memory - creates a string builder
		stB.append("Bob ");
		stB.append("is ");
		stB.append("a ");
		stB.append("pretty man!");
		System.out.println(stB);
		
		stB.insert(7, "not ");
		System.out.println(stB);
		
		stB.delete(7, 11);
		System.out.println(stB);
	}
}
